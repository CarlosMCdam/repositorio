package ejercicios;

public class Ej4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,i2,c[][] = new int[10][10];
		for ( i = 0; i < c.length; i++ ) {
			if (i%2==0) {
				for ( i2 = 0; i2 < c.length; i2++ ) {
					c[i][i2] = 1;
					System.out.print(c[i][i2]);
				}
				System.out.println();
			}
			else {
				for ( i2 = 0; i2 < c.length; i2++ ) {
					c[i][i2] = 0;
					System.out.print(c[i][i2]);
				}
				System.out.println();
			}
		}
	}

}
