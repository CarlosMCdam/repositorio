package ejercicios;

public class Ej12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j,marco[][] = new int[5][15];
		System.out.println(marco[5].length);
		for ( i = 0; i < marco.length; i++) {
			for ( j = 0; j < marco[j].length; j++) {
				if ( i==0 || i==marco.length-1 || j==0 || j==14) {
					System.out.print("1");
				}
				else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}

}
