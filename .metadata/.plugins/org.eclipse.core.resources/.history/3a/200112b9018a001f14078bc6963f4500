package prueba;

import java.util.Scanner;

public class ej10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		float c;
		for (c = 0f;c<32f && c>255f;System.out.println("asd")) {
		System.out.println("Introduzca un n√∫mero entre 32 y 255");
		Scanner l = new Scanner(System.in);
		c = l.nextInt();
		}
		System.out.println((char)c);
	}

}
